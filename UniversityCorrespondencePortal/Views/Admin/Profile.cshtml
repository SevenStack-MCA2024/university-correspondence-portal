@model UniversityCorrespondencePortal.Models.Admin

@{
    ViewBag.Title = "Admin Profile";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Admin Profile</h2>

<table class="table table-bordered">
    <tr>
        <th>Admin ID</th>
        <td>@Model.AdminID</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Phone</th>
        <td>@Model.Phone</td>
    </tr>
    <tr>
        <th>Role</th>
        <td>@Model.Role</td>
    </tr>
</table>

<a href="@Url.Action("Dashboard", "Admin")" class="btn btn-primary">Back to Dashboard</a>